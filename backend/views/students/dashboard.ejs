<style>
  body {
    background-image: url('http://localhost:5002/images/background.jpg');
    background-repeat: no-repeat;
    background-size: cover;
  }
</style>

<body>

  <%- include('../utils/nav-bars/studentNavBar.ejs', { active: 'home' }) %>

    <main class="container my-4">
      <div class="row">
        <!-- <div class="col-md-4">
          <div class="card mb-3" style="background-color: rgb(96, 96, 96); color: white;">
            <div class="card-body d-flex align-items-center">
              <i class="fas fa-book fa-3x me-3"></i>
              <h2 class="card-title"></h2>
            </div>
          </div>
        </div> -->
        <div class="col-md-4">
          <div class="card mb-3" style="background-color: rgb(96, 96, 96); color: white;">
            <div class="card-body d-flex align-items-center justify-content-between">
              <h2 class="card-title">Visualize</h2>
              <a href="/student/visualize" class="btn btn-primary"><i class="fas fa-arrow-right"></i></a>
            </div>
          </div>
        </div>
        <!-- <div class="col-md-4">
          <div class="card mb-3" style="background-color: rgb(96, 96, 96); color: white;">
            <div class="card-body d-flex align-items-center justify-content-end">
              <i class="fas fa-cog fa-3x"></i>
            </div>
          </div>
        </div> -->
      </div>
    </main>

    <div class="con-row">
      <div class="con-card card1">
        <h4>Open Registrations</h4>
        <div class="list-content" style="background-color: rgb(96, 96, 96); color: white;">
          <ul class="list-group">
            <% for (let i=0; i < regis.length; i++) { %>
              <%- include('../utils/regisTile.ejs', { dept: regis[i].dept, sem: regis[i].sem, startDate: regis[i].startDate, endDate: regis[i].endDate }) %>
            <% } %>
            <li class="list-group-item list-item" style="background-color: rgb(96, 96, 96); color: white;">
              <p>Department: <span class="badge alert-info">CSE</span>
                Semester: <span class="badge alert-success">IV</span><br>
                Open from <span class="badge alert-warning">25-jan-2023</span> - <span
                  class="badge alert-danger">30-may-2023</span>
              </p>
            </li>
          </ul>
        </div>
      </div>
      <div class="con-card card2">
        <h4>Announcements</h4>
        <br>
        <div id="announcements">
          <% for (let i=0; i < announcements.length; i++) { %>
            <%- include('../utils/announcementTile.ejs', { text: announcements[i].announcement }) %>
          <% } %>
          <div class="alert announce-tile" role="alert">
            This is a danger alertâ€”check it out!
          </div>
        </div>
      </div>
    </div>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/5.0.2/js/bootstrap.bundle.min.js"></script>
    <script>
    </script>
</body>

